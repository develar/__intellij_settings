<application>
  <component name="CommonProfilerUISettings">
    <option name="callTreeState">
      <call-tree-ui>
        <option name="showCalleesList" value="false" />
      </call-tree-ui>
    </option>
    <option name="flameGraphState">
      <flame-graph-ui>
        <option name="experimentalStyleForNewUi" value="true" />
        <option name="navigateWithSingleClick" value="false" />
      </flame-graph-ui>
    </option>
    <option name="profileDuringDebugOptions">
      <profile-during-debug>
        <option name="enableRunToCursor" value="true" />
      </profile-during-debug>
    </option>
    <option name="runToolbarWidgetOptions">
      <run-toolbar-widget>
        <option name="show" value="false" />
      </run-toolbar-widget>
    </option>
    <option name="threadListOptions">
      <thread-list>
        <option name="showMetrics">
          <map>
            <entry key="threadIdMetric" value="false" />
          </map>
        </option>
        <option name="sortMetrics">
          <map>
            <entry key="allocationSize" value="true" />
            <entry key="executionTime" value="true" />
            <entry key="samplesCountMetric" value="true" />
          </map>
        </option>
      </thread-list>
    </option>
  </component>
</application>