<toolSet name="External Tools">
  <tool name="gulp" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="gulp" />
      <option name="PARAMETERS" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
  <tool name="docker-compose" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/bin/sh" />
      <option name="PARAMETERS" value="-c &quot;/usr/local/bin/docker-compose start $SelectedText$ &amp;&amp; /usr/local/bin/docker-compose logs $SelectedText$&quot;" />
      <option name="WORKING_DIRECTORY" value="$USER_HOME$/Documents/cloud-intellij" />
    </exec>
  </tool>
  <tool name="webpack" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="webpack" />
      <option name="PARAMETERS" value="--watch --colors --progress" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
</toolSet>